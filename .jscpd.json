{
  "threshold": 0,
  "minTokens": 40,
  "reporters": ["console", "json", "html"],
  "output": "logs/jscpd-report",
  "format": [
    "c",
    "h",
    "ls"
  ],
  "ignore": [
    "**/.git/**",
    "**/.libs/**",
    "**/node_modules/**",
    "**/*.o",
